@font-face
    font-family "Gellix"
    src url("/developer-portal/assets/fonts/Gellix-Light.eot")
    src url("/developer-portal/assets/fonts/Gellix-Light.woff2") format("woff2"),
    url("/developer-portal/assets/fonts/Gellix-Light.woff") format("woff"),
    url("/developer-portal/assets/fonts/Gellix-Light.ttf") format("truetype"),
    url("/developer-portal/assets/fonts/Gellix-Light.svg#Gellix-Light") format("svg"),
    url("/developer-portal/assets/fonts/Gellix-Light.eot?#iefix") format("embedded-opentype")
    font-weight 200
    font-style normal

